<DialogPane fx:id="dialogPane" prefHeight="242.0" prefWidth="397.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.bakerydb.frontend.controllers.InventoryEditorController">
   <buttonTypes>
      <ButtonType fx:constant="OK" />
      <ButtonType fx:constant="CANCEL" />
   </buttonTypes>
   <content>
      <GridPane alignment="TOP_CENTER" hgap="10.0" prefHeight="194.0" prefWidth="438.0" vgap="10.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="220.0" minWidth="10.0" prefWidth="104.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="383.0" minWidth="10.0" prefWidth="223.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
         </rowConstraints>
         <children>
            <TextField fx:id="nameTextField" GridPane.columnIndex="1" />
            <TextField fx:id="quantityTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="unitTextField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="reorderLevelTextField" prefHeight="0.0" prefWidth="224.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label text="Name" />
            <Label text="Quantity" GridPane.rowIndex="1" />
            <Label text="Unit" GridPane.rowIndex="2" />
            <Label text="Reorder Level" GridPane.rowIndex="3" />
         </children>
         <padding>
            <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
         </padding>
      </GridPane>
   </content>
</DialogPane>
